        "type": "git",
        "url": "https://github.com/blikenoother/dcrawler.git"
    },
    "dependencies": {
        "async": "0.9.0",
        "buffer-crc32": "0.2.3",
        "request": "2.45.0",
        "cheerio": "0.17.0",
        "lodash": "2.4.1",
